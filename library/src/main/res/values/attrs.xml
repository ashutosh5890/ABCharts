<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PieChart">
        <attr name="showChartText" format="boolean" />
        <attr name="labelX" format="float" />
        <attr name="labelY" format="float" />
        <attr name="labelWidth" format="float"/>
        <attr name="labelHeight" format="float"/>
        <attr name="labelColor" format="color"/>
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
        <attr name="highlightStrength" format="float"/>
        <attr name="pieRotation" format="float"/>
        <attr name="pointerRadius" format="float"/>
        <attr name="autoCenterPointerInSlice" format="boolean"/>

    </declare-styleable>
</resources>
